{
  "name": "nxplus",
  "description": "Utilities for Nx Workspace",
  "version": "1.1.0",
  "author": "Bram Borggreve @beeman",
  "bin": {
    "nxplus": "./bin/run"
  },
  "bugs": "https://github.com/beeman/nxplus/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "^4.0.0",
    "degit": "^2.8.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "replace-in-file": "^6.0.0",
    "tmp": "^0.2.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/tmp": "^0.2.0",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "globby": "^10",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^5",
    "nyc": "^14",
    "prettier": "^2.0.5",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/beeman/nxplus",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nxplus",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "beeman/nxplus",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "yarn prepack && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,graphql}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
